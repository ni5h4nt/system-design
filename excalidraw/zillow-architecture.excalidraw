    {
      "id": "sec4-content",
      "type": "text",
      "x": 680,
      "y": 830,
      "width": 550,
      "height": 210,
      "text": "GET /search?bbox=sw_lat,sw_lng,ne_lat,ne_lng\n    &status=for_sale&beds_min=3&price_max=600000\n  -> {properties: [{zpid, lat, lng, price, beds,\n      baths, thumbnail}], total_count}\n\nGET /property/:zpid\n  -> {address, attributes, zestimate, zestimate_history,\n      tax_history, sale_history, photos[], comps[],\n      schools[], neighborhood_stats}\n\nPOST /leads\n  {property_id, user_name, email, phone, message}\n  -> {lead_id, assigned_agent, status}\n\nPOST /saved-searches\n  {geo_boundary, filters, notification_prefs}\n  -> {search_id}\n\nGET /zestimate/:zpid\n  -> {value, confidence_range, rent_estimate, as_of}",
