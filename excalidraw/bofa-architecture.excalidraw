    {
      "id": "sec3-content",
      "type": "text",
      "x": 70,
      "y": 830,
      "width": 540,
      "height": 210,
      "text": "Account\n  account_id PK, customer_id FK, product_id FK,\n  status (active|dormant|frozen|closed), currency\n\nAccountBalance\n  account_id FK, balance_type (CURRENT|AVAILABLE|\n  HELD|PENDING), amount DECIMAL(18,2), as_of\n\nLedgerEntry  (append-only, immutable)\n  entry_id PK, transaction_id UUID, account_id FK,\n  entry_type (debit|credit), amount, effective_date,\n  posting_date, source_system, reference_id\n\nProductDefinition\n  product_id PK, type, interest_rules, fee_schedule",
