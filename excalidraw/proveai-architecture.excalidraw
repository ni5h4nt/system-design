    {"id":"re-key-insight","type":"rectangle","x":80,"y":2880,"width":1120,"height":50,"strokeColor":"#862e9c","backgroundColor":"#f8f0fc","fillStyle":"solid","strokeWidth":2,"roughness":1,"opacity":100,"angle":0,"seed":380,"version":1,"versionNonce":380,"isDeleted":false,"boundElements":null,"updated":1,"link":null,"locked":false,"roundness":{"type":3},"groupIds":[]},
    {"id":"re-key-insight-text","type":"text","x":100,"y":2888,"width":1080,"height":35,"text":"KEY: Engine SUGGESTS, never auto-fixes. Operates on STRUCTURED telemetry (not open-ended text). Fixed taxonomy of 8 failure types\nbounds nondeterminism. Each resolved case makes the system smarter. Wrong suggestions feed negative signal. The irony: using GenAI to debug GenAI.","fontSize":13,"fontFamily":1,"textAlign":"center","strokeColor":"#862e9c","backgroundColor":"transparent","fillStyle":"solid","strokeWidth":1,"roughness":1,"opacity":100,"angle":0,"seed":381,"version":1,"versionNonce":381,"isDeleted":false,"boundElements":null,"updated":1,"link":null,"locked":false,"groupIds":[]},

    {"id":"re-flow-label","type":"text","x":80,"y":2950,"width":1120,"height":50,"text":"FLOW: Anomaly Detected  [1] Collect Evidence  [2] Classify Failure  [3] Analyze Root Cause  [4] Suggest Fix\n          [5] Engineer Reviews & Applies  [6] Resolution  Knowledge Base  [3] (reinforcement loop)","fontSize":14,"fontFamily":1,"textAlign":"center","strokeColor":"#1e1e1e","backgroundColor":"transparent","fillStyle":"solid","strokeWidth":1,"roughness":1,"opacity":100,"angle":0,"seed":382,"version":1,"versionNonce":382,"isDeleted":false,"boundElements":null,"updated":1,"link":null,"locked":false,"groupIds":[]},

    {"id":"cap-title","type":"text","x":250,"y":3050,"width":800,"height":35,"text":"9. DEEP DIVE: Consistency vs Availability  CAP Trade-offs","fontSize":22,"fontFamily":1,"textAlign":"center","strokeColor":"#1e1e1e","backgroundColor":"transparent","fillStyle":"solid","strokeWidth":1,"roughness":1,"opacity":100,"angle":0,"seed":400,"version":1,"versionNonce":400,"isDeleted":false,"boundElements":null,"updated":1,"link":null,"locked":false,"groupIds":[]},

