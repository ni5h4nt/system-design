      "text": "GET /.well-known/agent.json\n{\n  \"name\": \"Hotel Booking Agent\",\n  \"url\": \"https://hotel-agent.example/a2a\",\n  \"protocolVersion\": \"0.3\",\n  \"capabilities\": {\n    \"streaming\": true,\n    \"pushNotifications\": true,\n    \"stateTransitionHistory\": true\n  },\n  \"skills\": [{\n    \"id\": \"book_hotel\",\n    \"name\": \"Book Hotel\",\n    \"tags\": [\"travel\", \"booking\"]\n  }],\n  \"defaultInputModes\": [\"text/plain\", \"application/json\"],\n  \"defaultOutputModes\": [\"text/plain\", \"application/pdf\"],\n  \"authentication\": {\n    \"schemes\": [{\"scheme\": \"OAuth2\", ...}]\n  }\n}",
